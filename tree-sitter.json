{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "metal",
      "camelcase": "Metal",
      "title": "Metal",
      "scope": "source.metal",
      "file-types": [
        "metal"
      ],
      "injection-regex": "^metal$",
      "class-name": "TreeSitterMetal"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "tree-sitter grammar for Metal Shading Langage",
    "authors": [
      {
        "name": "Sasha Krassovsky",
        "email": "krassovskysasha@gmail.com",
        "url": "https://save-buffer.github.io/"
      }
    ],
    "links": {
      "repository": "https://github.com/save-buffer/tree-sitter-metal"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}